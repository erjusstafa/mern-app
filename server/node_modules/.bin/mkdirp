#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules/mkdirp/bin/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules/mkdirp/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/node_modules:/mnt/c/Users/stafa/Desktop/node_modules:/mnt/c/Users/stafa/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules/mkdirp/bin/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules/mkdirp/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/node_modules:/mnt/c/Users/stafa/Desktop/node_modules:/mnt/c/Users/stafa/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/stafa/Desktop/mern-app/server/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mkdirp/bin/cmd.js" "$@"
else
  exec node  "$basedir/../mkdirp/bin/cmd.js" "$@"
fi
